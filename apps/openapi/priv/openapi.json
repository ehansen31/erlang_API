          "description" : "ID of account to return",
          "in" : "path",
          "name" : "userId",
          "required" : true,
          "schema" : {
            "format" : "int",
            "type" : "integer"
          }
        } ],
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/User"
                }
              }
            },
            "description" : "successful operation"
          },
          "400" : {
            "content" : { },
            "description" : "Invalid ID supplied"
          },
          "404" : {
            "content" : { },
            "description" : "Account not found"
          }
        },
        "summary" : "Get an existing user",
        "tags" : [ "user" ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "User" : {
        "description" : "A User who is purchasing from the pet store",
        "example" : {
          "firstName" : "firstName",
          "lastName" : "lastName",
          "password" : "password",
          "userStatus" : 6,
          "phone" : "phone",
          "id" : 0,
          "email" : "email",
          "username" : "username"
        },
        "properties" : {
          "id" : {
